<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ChannelServlet</servlet-name>
		<servlet-class>door_man.ChannelServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChannelServlet</servlet-name>
		<url-pattern>/gettoken/*</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
	    <servlet-name>EstatusServlet</servlet-name>
	    <servlet-class>door_man.EstatusServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>EstatusServlet</servlet-name>
	    <url-pattern>/estatus/*</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
	    <servlet-name>WriteCardServlet</servlet-name>
	    <servlet-class>door_man.WriteCardServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>WriteCardServlet</servlet-name>
	    <url-pattern>/writecard/*</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
	    <servlet-name>SyncServlet</servlet-name>
	    <servlet-class>door_man.SyncServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>SyncServlet</servlet-name>
	    <url-pattern>/sync/*</url-pattern>
	</servlet-mapping>
	
		
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
